kops create cluster test-cluster.demirozu.click --state=s3://kops-state-ninova \
--zones=us-east-1a,us-east-1b --node-count=2 --node-size=t3.small \
--node-volume-size=8 --control-plane-size=t3.medium --control-plane-volume-size=8 \
--dns-zone=demirozu.click 

kops edit cluster --name test-cluster.demirozu.click --state=s3://kops-state-ninova

kops update cluster --name test-cluster.demirozu.click --state s3://kops-state-ninova --yes --admin

kops validate cluster --wait 10m --state=s3://kops-state-ninova

kops delete cluster --name test-cluster.demirozu.click --state s3://kops-state-ninova --yes
